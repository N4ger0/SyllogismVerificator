<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>


<TitledPane xmlns:fx="http://javafx.com/fxml/1" collapsible="false" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
            text="Syllogism solver"
            fx:controller="univ.syllogismverificator.controllers.MainMenuController"
            xmlns="http://javafx.com/javafx/21"
            fx:id="titled_pane">
    <content>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
            <children>
                <Button fx:id="buttonSolver" alignment="CENTER" mnemonicParsing="false" prefHeight="86.0"
                        prefWidth="148.0" text="Solve syllogism" textAlignment="CENTER"
                        onAction="#handleClickButtonSolver"/>
                <Region prefHeight="30.0" prefWidth="577.0"/>
                <Button fx:id="ButtonTab" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="87.0"
                        prefWidth="150.0" text="All possible syllogism"
                        onAction="#handleClickButtonTable"/>
            </children>
        </VBox>
    </content>
</TitledPane>
