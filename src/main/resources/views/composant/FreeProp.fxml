<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="univ.syllogismverificator.controllers.composant.FreePropController"
      alignment="CENTER" prefHeight="49.0" prefWidth="598.0" spacing="20.0">
    <Text HBox.hgrow="ALWAYS" fx:id="text"/>

    <MenuButton mnemonicParsing="false" text="QQL" prefWidth="75" fx:id="freeQQL"/>
    <MenuButton mnemonicParsing="false" text="Terme 1" prefWidth="75" fx:id="freeTerme1"/>
    <MenuButton mnemonicParsing="false" text="Terme 2" prefWidth="75" fx:id="freeTerme2"/>

    <VBox fx:id="freeMediumTerm">
        <Text fx:id="freeMediumTermText" text="Middle Term"/>
        <TextField fx:id="freeTextFieldMedium" prefWidth="180.0"/>
    </VBox>
</HBox>